#include<stdio.h>
int main(){
    int n,total=0,sum=0;
    printf("enter no. of elements:");
    scanf("%d",&n);
    int a[n];
    printf("enter elements:");
    for( int i=0;i<n-1;i++)
    {
        scanf("%d",&a[i]);
        sum=sum+a[i];
    }
    total=(n*(n+1))/2;
    printf("%d",total-sum);
    return 0;
}
